{
  "name": "gatsby-themes",
  "version": "1.0.1",
  "description": "A collection of Gatsby themes",
  "author": "Trevor W. Adams <trevorwadams@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn workspace design-system-site build",
    "build:default": "yarn workspace default-site build",
    "build:purple": "yarn workspace purple-site build",
    "build:legacy": "yarn workspace legacy-project build",
    "clean": "yarn workspaces run clean",
    "start": "yarn workspace design-system-site start",
    "start:default": "yarn workspace default-site start",
    "start:docs": "yarn workspace docs-site start",
    "start:purple": "yarn workspace purple-site start",
    "start:legacy": "yarn workspace legacy-project start",
    "serve": "yarn workspace design-system-site serve",
    "serve:default": "yarn workspace default-site serve",
    "serve:purple": "yarn workspace purple-site serve",
    "serve:legacy": "yarn workspace legacy-project serve"
  },
  "workspaces": [
    "sites/*",
    "themes/*"
  ]
}
